<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <font
        android:font="@font/noto_hesita_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/noto_hesita_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:ignore="UnusedAttribute"/>

    <font
        android:font="@font/noto_hesita_light"
        android:fontStyle="normal"
        android:fontWeight="200"
        app:font="@font/noto_hesita_light"
        app:fontStyle="normal"
        app:fontWeight="200"
        tools:ignore="UnusedAttribute"/>

    <font
        android:font="@font/noto_hesita_demilight"
        android:fontStyle="normal"
        android:fontWeight="300"
        app:font="@font/noto_hesita_demilight"
        app:fontStyle="normal"
        app:fontWeight="300"
        tools:ignore="UnusedAttribute"/>


    <font
        android:font="@font/noto_hesita_medium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/noto_hesita_medium"
        app:fontStyle="normal"
        app:fontWeight="500"
        tools:ignore="UnusedAttribute"/>

    <font
        android:font="@font/noto_hestia_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/noto_hestia_bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        tools:ignore="UnusedAttribute"/>

    <font
        android:font="@font/noto_hestia_black"
        android:fontStyle="normal"
        android:fontWeight="800"
        app:font="@font/noto_hestia_black"
        app:fontStyle="normal"
        app:fontWeight="800"
        tools:ignore="UnusedAttribute"/>
</font-family>